{
  "LOG": false,
  "LOG_LEVEL": "info",
  "CLAUDE_PATH": "",
  "HOST": "127.0.0.1",
  "PORT": 3456,
  "APIKEY": "sk-anything",
  "API_TIMEOUT_MS": "600000",
  "PROXY_URL": "",
  "transformers": [
    {
      "name": "",
      "path": "./transformers/glm-thinking.js",
      "options": {
        "enabled": "true",
        "debug": "true"
      }
    }
  ],
  "Providers": [
    {
      "name": "haiku-minimax",
      "api_base_url": "https://api.minimaxi.com/anthropic/v1/messages",
      "api_key": "YOUR_MINIMAX_API_KEY",
      "models": [
        "MiniMax-M2"
      ],
      "transformer": {
        "use": [
          "Anthropic"
        ]
      }
    },
    {
      "name": "sonnet-minimax",
      "api_base_url": "https://api.minimaxi.com/anthropic/v1/messages",
      "api_key": "YOUR_MINIMAX_API_KEY",
      "models": [
        "MiniMax-M2"
      ],
      "transformer": {
        "use": [
          "Anthropic"
        ]
      }
    },
    {
      "name": "opus-minimax",
      "api_base_url": "https://api.minimaxi.com/anthropic/v1/messages",
      "api_key": "YOUR_MINIMAX_API_KEY",
      "models": [
        "MiniMax-M2"
      ],
      "transformer": {
        "use": [
          "Anthropic"
        ]
      }
    },
    {
      "name": "haiku-glm",
      "api_base_url": "https://open.bigmodel.cn/api/anthropic/v1/messages",
      "api_key": "YOUR_GLM_API_KEY",
      "models": [
        "glm-4.6"
      ],
      "transformer": {
        "use": [
          "Anthropic"
        ]
      }
    },
    {
      "name": "sonnet-glm",
      "api_base_url": "https://open.bigmodel.cn/api/anthropic/v1/messages",
      "api_key": "YOUR_GLM_API_KEY",
      "models": [
        "glm-4.6"
      ],
      "transformer": {
        "use": [
          "Anthropic"
        ]
      }
    },
    {
      "name": "opus-glm",
      "api_base_url": "https://api.z.ai/api/coding/paas/v4/chat/completions",
      "api_key": "YOUR_GLM_API_KEY",
      "models": [
        "glm-4.6"
      ],
      "transformer": {
        "use": [
          "reasoning",
          "glm-thinking"
        ]
      }
    },
    {
      "name": "opus-openrouter",
      "api_base_url": "https://openrouter.ai/api/v1/chat/completions",
      "api_key": "api-key",
      "models": [
        "anthropic/claude-4.5-sonnet:thinking",
        "google/gemini-2.5-pro-preview"
      ],
      "transformer": {
        "use": [
          "openrouter"
        ]
      }
    }
  ],
  "Router": {
    "engine": "unified",
    "defaultRoute": "sonnet",
    "rules": [
      {
        "name": "用户指定规则",
        "condition": {
          "type": "custom",
          "customFunction": "modelContainsComma"
        },
        "action": {
          "route": "${userModel}",
          "description": "用户指定路由：用户在请求中直接指定provider,model格式"
        },
        "priority": 200,
        "enabled": true
      },
      {
        "name": "提供商映射规则",
        "condition": {
          "type": "custom",
          "customFunction": "directModelMapping"
        },
        "action": {
          "route": "${mappedModel}",
          "description": "提供商映射：将用户指定的模型名映射到对应的provider格式"
        },
        "priority": 190,
        "enabled": true
      },
      {
        "name": "长上下文规则",
        "condition": {
          "type": "tokenThreshold",
          "value": 180000,
          "operator": "gt"
        },
        "action": {
          "route": "sonnet",
          "description": "长上下文路由：基于token阈值选择模型"
        },
        "priority": 100,
        "enabled": true
      },
      {
        "name": "子代理规则",
        "condition": {
          "type": "fieldExists",
          "field": "system.1.text",
          "value": "<CCR-SUBAGENT-MODEL>",
          "operator": "contains"
        },
        "action": {
          "route": "${subagent}",
          "description": "子代理路由：通过特殊标记选择模型"
        },
        "priority": 90,
        "enabled": true
      },
      {
        "name": "后台规则",
        "condition": {
          "type": "modelContains",
          "value": "haiku",
          "operator": "contains"
        },
        "action": {
          "route": "haiku",
          "description": "后台路由：Haiku模型自动使用轻量级模型"
        },
        "priority": 80,
        "enabled": true
      },
      {
        "name": "网络搜索规则",
        "condition": {
          "type": "toolExists",
          "value": "web_search",
          "operator": "exists"
        },
        "action": {
          "route": "sonnet",
          "description": "网络搜索路由：检测到web_search工具时使用特定模型"
        },
        "priority": 70,
        "enabled": true
      },
      {
        "name": "思考规则",
        "condition": {
          "type": "fieldExists",
          "field": "thinking",
          "value": true,
          "operator": "eq"
        },
        "action": {
          "route": "opus",
          "description": "思考路由：启用thinking参数时使用特定模型"
        },
        "priority": 60,
        "enabled": true
      }
    ],
    "contextThreshold": {
      "default": 60000,
      "longContext": 180000
    },
    "cache": {
      "enabled": true,
      "ttl": 300000,
      "maxSize": 1000
    },
    "debug": {
      "enabled": true,
      "logLevel": "info"
    }
  },
  "StatusLine": {
    "enabled": false,
    "currentStyle": "default",
    "default": {
      "modules": []
    },
    "powerline": {
      "modules": []
    }
  },
  "CUSTOM_ROUTER_PATH": "",
  "OPENAI_API_KEY": "",
  "OPENAI_BASE_URL": "",
  "OPENAI_MODEL": ""
}